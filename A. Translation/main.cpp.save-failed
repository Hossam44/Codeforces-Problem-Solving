#include <iostream>
#include<cstring>
using namespace std;

int main()
{
    char arr[100];
    char arr1[100],w;
    int n,q,t;
    cin>>arr;
    cin>>arr;
    cin>>arr1;
    q=strlen(arr);
    t=q-1;
    for(int i=0;i<q;i++)
    {
        if(q/2>i)
        {
            w=arr[i];
            arr[i]=arr[t];
            arr[t]=w;
        }
        t--;
    }
    cout<<arr;
    return 0;
}
